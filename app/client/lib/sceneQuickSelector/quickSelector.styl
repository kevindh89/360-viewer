#quickSelector {
  z-index: 100;
  height: 100%;
  overflow-y: scroll;
}
#quickSelector img {
  width: 50%;
  display: inline-block;
  margin-top: 55px;
  border-radius: 25px;
}

@media (max-width: 500px) {
  #quickSelector {
    margin-top: 70px;
  }
  #quickSelector img {
    margin-top: 10px;
  }
  #quickSelector .col-xs-4 {
    width: 100%;
  }
}