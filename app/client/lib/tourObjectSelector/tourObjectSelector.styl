#tourObjectSelector {
  z-index: 100;
  height: 100%;
  overflow-y: scroll;
}
#tourObjectSelector img {
  width: 50%;
  display: inline-block;
  margin-top: 55px;
  border-radius: 25px;
}

@media (max-width: 500px) {
  #tourObjectSelector {
    margin-top: 70px;
  }
  #tourObjectSelector img {
    margin-top: 10px;
  }
  #tourObjectSelector .col-xs-4 {
    width: 100%;
  }
}
